#define _CRT_SECURE_NO_WARNINGS 1


//递归初步理解
#include<stdio.h>

void add(int n)
{
	if (n < 10)
	{
		add(n+1);
	}
	printf("%d ", n);
}

int main()
{
	int i = 0;
	add(i);
	return 0;
}
//递归初步理解


