#define _CRT_SECURE_NO_WARNINGS 1


//函数实现乘法表n*n
#include<stdio.h>
#include<string.h>

void mul_tab(int x, int y)
{
	int i = 0;
	int j = 0;
	for (i = 1; i <= x; i++)
	{
		for (j = 1; j <= i; j++)
		{
			int sum = j*i;
			printf("%2d*%-2d=%-2d ", j, i, sum);
		}
		printf("\n");
	}
}
int main()
{
	int i = 0;
	scanf("%d", &i);
	int j = i;
	mul_tab(i, j);
	return 0;
}
//乘法表n*n
